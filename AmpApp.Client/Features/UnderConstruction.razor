@page "/uc"
@page "/uc/{pagename}"
@page "/uc/{pagename}/{id}"

<AmpPageTitle Title="@PageName" />

<p>This page is under construction.</p>
@if (Id != null)
{
    <p style="margin-top: 10px"><i>@Id</i></p>
}

@code {
    [Parameter] public string PageName { get; set; } = "Under Construction";
    [Parameter] public string? Id { get; set; }
}
